        android:layout_marginTop="352dp"
        android:layout_marginBottom="3dp"
        android:background="@color/colorTransparent"
        app:layout_constraintBottom_toTopOf="@+id/egg3"
        app:layout_constraintStart_toEndOf="@+id/location4_3"
